{
  "name": "tldraw-vue",
  "version": "0.0.1",
  "description": "A vue 3 wrapper for tldraw",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "build:no-clean": "vite build",
    "serve": "vite serve",
    "test": "echo \"Error: no test specified\"",
    "release": "np",
    "version": "pnpm run build"
  },
  "keywords": [
    "tldraw",
    "vue"
  ],
  "author": "Artesa",
  "license": "MIT",
  "packageManager": "pnpm@9.15.4",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/tldraw-vue.js",
  "module": "./dist/tldraw-vue.js",
  "exports": {
    ".": {
      "import": "./dist/tldraw-vue.js",
      "default": "./dist/tldraw-vue.js"
    },
    "./tldraw-vue.css": {
      "import": "./dist/tldraw-vue.css",
      "require": "./dist/tldraw-vue.css"
    },
    "./dist/tldraw-vue.css": {
      "import": "./dist/tldraw-vue.css",
      "require": "./dist/tldraw-vue.css"
    }
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@tldraw/tldraw": "3.9.0",
    "fast-copy": "3.0.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/tsconfig": "0.7.0",
    "@vueuse/core": "12.8.1",
    "autoprefixer": "10.4.20",
    "fast-array-diff": "^1.1.0",
    "np": "^10.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.8.2",
    "vite": "6.2.0",
    "vite-plugin-dts": "4.5.3",
    "vite-plugin-qrcode": "0.2.4",
    "vue": "3.5.13"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.5.0",
    "vue": "^3.3.8"
  }
}
